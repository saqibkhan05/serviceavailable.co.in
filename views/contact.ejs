<%- include('req/header') %>


<div style="margin-top: 50px; margin-bottom: 50px;">
  <h1 class="display-4" align="center">Get in touch</h1>
  <p align="center" > fill the contact us form and get in touch with us..</p>
  <hr align="center" width="50%">
</div>

<div class="container">
  <form action="/contact" method="post" onsubmit="myfun()">
    <div class="form-group">
      <label for="exampleInputEmail1">Name</label>
      <input type="text" name="username" class="form-control">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Email address</label>
      <input type="text" name="email" class="form-control" >
    </div>
    <div class="form-group">
      <label for="exampleFormControlTextarea1">Your message.</label>
        <textarea class="form-control" name="msg" rows="5"></textarea>
      </div>
    
    <!-- <input type="hidden" name="_csrf" value="<%= usertoken %>"> -->
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
<script>
  function myfun() {
    Swal.fire(
  'Good job!',
  'You clicked the button!',
  'success'
  )
  }
</script>


